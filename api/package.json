{
  "name": "api",
  "version": "1.0.0",
  "description": "Express API for MedicalArtists",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "preinstall": "bun x only-allow bun || true",
    "dev": "bun --hot src/server.ts",
    "build": "bun run prisma:generate && bun run tsc",
    "start": "bun dist/server.js",
    "lint": "bun x eslint 'src/**/*.ts'",
    "prisma:generate": "bun prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@prisma/client": "^5.20.0",
    "@trpc/server": "11.6.0",
    "@types/cors": "^2.8.19",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.1.20",
  "ts-node": {
    "esm": true
  }
}
