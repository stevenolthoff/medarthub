  "main": "index.ts",
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:web": "npm --prefix client run dev",
    "dev": "concurrently -k -n API,WEB \"npm:dev:api\" \"npm:dev:web\"",

    "build:web": "npm --prefix client run build",
    "build:api": "tsc",
    "build": "npm run build:web && npm run build:api",

    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "npm": "^11.6.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "ts-node": {
    "esm": true
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "crypto": "^1.0.1"
  }
}
